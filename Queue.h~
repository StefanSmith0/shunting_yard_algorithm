#ifndef QUEUE_H
#define QUEUE_H
#include <iostream>

using namespace std;

class Queue {
 public:
  Queue();
  ~Queue();
  void destroy(data* current);
  void enqueue(char newValue);
  char dequeue();
 private:
  struct data {
    char value;
    data* nextData;
  };
  int size;
  data* head;
};

#endif
